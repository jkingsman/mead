{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": ["error", 4]
    },
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {
        "meadData": true,
        "Materialize": false,
        "Handlebars": false,
        "recipes": true,
        "kjua": false,
        "showdown": false
    }
}
