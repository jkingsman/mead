{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": ["error", 4],
        "no-underscore-dangle": [0],
        "no-restricted-syntax": [0],
        "no-restricted-globals": [0],
        "guard-for-in": [0]
    },
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {
        "meadData": true,
        "Materialize": false,
        "Handlebars": false,
        "recipes": true,
        "kjua": false,
        "showdown": false,
        "initializeMaterialize": false
    }
}
